{
  "name": "simon-js-tool",
  "version": "4.2.1",
  "packageManager": "pnpm@7.6.0",
  "description": "js-tool",
  "author": "Simon He",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git + git@github.com:Simon-He95/simon-js-tool.git"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/simon-js-tool/issues"
  },
  "keywords": [
    "simon-js-tool",
    "utils"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkgroll --minify",
    "dev": "pkgroll --watch",
    "play": "pnpm run -C playground dev",
    "play:build": "pnpm run -C playground build",
    "serve": "pnpm run -C playground serve",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 --cache",
    "lint:fix": "pnpm run lint --fix",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest -u",
    "test:e2e": "cypress open",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --tag --push && pnpm publish"
  },
  "dependencies": {
    "@vueuse/core": "^8.9.4",
    "htmlparser2": "^8.0.1",
    "qrcode": "^1.5.1",
    "spark-md5": "^3.0.2",
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.3",
    "@types/node": "^17.0.45",
    "@types/qrcode": "^1.5.0",
    "@types/spark-md5": "^3.0.2",
    "@vitejs/plugin-vue": "^2.3.4",
    "bumpp": "^7.2.0",
    "eslint": "8.20.0",
    "pkgroll": "^1.4.0",
    "simon-js-tool": "workspace:^4.1.12",
    "typescript": "^4.8.4",
    "vite": "^2.9.15",
    "vite-plugin-pages": "^0.22.0",
    "vitest": "^0.7.13"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
